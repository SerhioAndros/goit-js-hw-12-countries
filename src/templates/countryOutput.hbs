{{!-- {{#each countr}} --}}
<h1>{{name}}</h1>
<div class='output-info'>
  <div class='output-text-info'>
    <ul>
      <li class='list'><span class='accent'>Capital:</span> {{capital}}</li>
      <li class='list'><span class='accent'>Population:</span> {{population}}</li>
      <li class='list'><span class='accent'>Languages:</span>
        <ul>
          {{#each languages}}
            <li>{{name}}</li>
          {{/each}}
        </ul>
      </li>
    </ul>
  </div>
  <div class='output-img'>
    <img src='{{flag}}' alt='{{name}} flag' />
  </div>

</div>
{{!-- {{/each}} --}}